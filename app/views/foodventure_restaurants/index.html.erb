<p id="notice"><%= notice %></p>

<h1>Listing Foodventure Restaurants</h1>

<table>
  <thead>
    <tr>
      <th>Foodventure</th>
      <th>Restaurant</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @foodventure_restaurants.each do |foodventure_restaurant| %>
      <tr>
        <td><%= foodventure_restaurant.foodventure_id %></td>
        <td><%= foodventure_restaurant.restaurant_id %></td>
        <td><%= link_to 'Show', foodventure_restaurant %></td>
        <td><%= link_to 'Edit', edit_foodventure_restaurant_path(foodventure_restaurant) %></td>
        <td><%= link_to 'Destroy', foodventure_restaurant, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Foodventure restaurant', new_foodventure_restaurant_path %>
